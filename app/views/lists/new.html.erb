<div class="p-3">
<h1>New List</h1>
<%= simple_form_for(@list) do |f| %>
<div class="form-inputs">
  <%= f.input :name %>
  <%= f.input :photo, as: :file %>
</div>
<div class="form-actions btn btn-primary mb-4">
  <%= f.button :submit %>
</div>
<% end %>
 <%= link_to "Back to lists", lists_path%>
</div>
